{
  "openapi": "3.0.1",
  "info": {
    "version": "1.0.0",
    "title": "GOreITshniki",
    "description": "Api"
  },
  "servers": [
    {
      "url": "https://backend-project-dl3a.onrender.com/api",
      "description": "API base URL"
    }
  ],
  "tags": [
    {
      "name": "User",
      "description": "Authorization endpoints"
    },
    {
      "name": "Product"
    },
    {
      "name": "Exercise"
    }
  ],
  "paths": {
    "/users/register": {
      "post": {
        "tags": ["User"],
        "summary": "User registration",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "example": {
                "email": "user@example.com",
                "name": "Gore Itshnik",
                "password": "qwerty123"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "example": {
                  "email": "user@example.com",
                  "name": "Gore Itshnik",
                  "token": "string"
                }
              }
            }
          },
          "400": {
            "description": "Bad request (invalid request body)"
          },
          "409": {
            "description": "Email already in use"
          },
          "500": {
            "description": "Server error"
          }
        }
      }
    },
    "/users/login": {
      "post": {
        "tags": ["User"],
        "summary": "User login",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Successful operation"
          },
          "400": {
            "description": "Bad request (invalid request body)"
          },
          "401": {
            "description": "Email or password invalid"
          },
          "409": {
            "description": "Email already in use"
          },
          "500": {
            "description": "Server error"
          }
        }
      }
    },
    "/users/current": {
      "get": {
        "security": [
          {
            "Bearer": []
          }
        ],
        "tags": ["User"],
        "summary": "Get current user",
        "responses": {
          "201": {
            "description": "Successful operation"
          },
          "400": {
            "description": "Bad request (invalid request body)"
          },
          "401": {
            "description": "Email or password invalid"
          },
          "409": {
            "description": "Email already in use"
          },
          "500": {
            "description": "Server error"
          }
        }
      }
    },
    "/users/update": {
      "patch": {
        "security": [
          {
            "Bearer": []
          }
        ],
        "tags": ["User"],
        "summary": "Update user information",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "height": {
                    "type": "number"
                  },
                  "currentWeight": {
                    "type": "number"
                  },
                  "desiredWeight": {
                    "type": "number"
                  },
                  "birthday": {
                    "type": "string",
                    "format": "date"
                  },
                  "sex": {
                    "type": "string"
                  },
                  "blood": {
                    "type": "number"
                  },
                  "levelActivity": {
                    "type": "number"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Update success"
          }
        }
      }
    },
    "/users/avatar": {
      "patch": {
        "security": [
          {
            "Bearer": []
          }
        ],
        "tags": ["User"],
        "summary": "Update user avatar",
        "requestBody": {
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "avatarURL": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful operation"
          },
          "401": {
            "description": "Not authorized"
          },
          "500": {
            "description": "Server error"
          }
        }
      }
    },
    "/users/logout": {
      "post": {
        "security": [
          {
            "Bearer": []
          }
        ],
        "tags": ["User"],
        "summary": "User logout",
        "responses": {
          "204": {
            "description": "Logout success"
          }
        }
      }
    },
    "/products": {
      "get": {
        "security": [
          {
            "Bearer": []
          }
        ],
        "tags": ["Product"],
        "summary": "Get products",
        "parameters": [
          {
            "name": "search",
            "in": "query",
            "description": "Keyword",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "category",
            "in": "query",
            "description": "Category",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "recommendation",
            "in": "query",
            "description": "Type",
            "schema": {
              "type": "string",
              "enum": ["all", "recommended", "not recommended"],
              "default": "all"
            }
          },
          {
            "name": "page",
            "in": "query",
            "description": "Page",
            "schema": {
              "type": "string",
              "default": "1"
            }
          },
          {
            "name": "limit",
            "in": "query",
            "description": "Limit per page",
            "schema": {
              "type": "string",
              "default": "10"
            }
          }
        ],
        "responses": {
          "401": {
            "description": "Not authorized"
          },
          "500": {
            "description": "Server error"
          }
        }
      }
    },
    "/products/categories": {
      "get": {
        "security": [
          {
            "Bearer": []
          }
        ],
        "tags": ["Product"],
        "summary": "Get product categories",
        "responses": {
          "401": {
            "description": "Not authorized"
          },
          "500": {
            "description": "Server error"
          }
        }
      }
    },
    "/exercises": {
      "get": {
        "security": [
          {
            "Bearer": []
          }
        ],
        "tags": ["Exercise"],
        "summary": "Get exercises",
        "responses": {
          "200": {
            "description": "Exercises found"
          },
          "401": {
            "description": "Not authorized"
          },
          "404": {
            "description": "Not found"
          },
          "500": {
            "description": "Server error"
          }
        }
      }
    },
    "/exercises/bodyparts": {
      "get": {
        "security": [
          {
            "Bearer": []
          }
        ],
        "tags": ["Exercise"],
        "summary": "Get body parts exercises",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "example": {
                  "filter": "Body parts",
                  "name": "string",
                  "imgURL": "string"
                }
              }
            },
            "description": "Body parts exercises found"
          },
          "401": {
            "description": "Not authorized"
          },
          "404": {
            "description": "Not found"
          },
          "500": {
            "description": "Server error"
          }
        }
      }
    },
    "/exercises/muscles": {
      "get": {
        "security": [
          {
            "Bearer": []
          }
        ],
        "tags": ["Exercise"],
        "summary": "Get muscles exercises",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "example": {
                  "filter": "Muscles",
                  "name": "string",
                  "imgURL": "string"
                }
              }
            },
            "description": "Muscles exercises found"
          },
          "401": {
            "description": "Not authorized"
          },
          "404": {
            "description": "Not found"
          },
          "500": {
            "description": "Server error"
          }
        }
      }
    },
    "/exercises/equipment": {
      "get": {
        "security": [
          {
            "Bearer": []
          }
        ],
        "tags": ["Exercise"],
        "summary": "Get equipment exercises",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "example": {
                  "filter": "Equipment",
                  "name": "string",
                  "imgURL": "string"
                }
              }
            },
            "description": "Equipment exercises found"
          },
          "401": {
            "description": "Not authorized"
          },
          "404": {
            "description": "Not found"
          },
          "500": {
            "description": "Server error"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "User": {
        "type": "object",
        "required": ["email", "name", "password"],
        "properties": {
          "name": {
            "type": "string",
            "description": "User's name"
          },
          "email": {
            "type": "string",
            "description": "User's email",
            "format": "email"
          },
          "password": {
            "type": "string",
            "description": "User's password",
            "minLength": 6
          },
          "token": {
            "type": "string",
            "default": null
          },
          "avatarURL": {
            "type": "string",
            "minimum": 150,
            "default": ""
          },
          "height": {
            "type": "number",
            "default": null
          },
          "currentWeight": {
            "type": "number",
            "default": null,
            "minimum": 35
          },
          "desiredWeight": {
            "type": "number",
            "default": null,
            "minimum": 35
          },
          "birthday": {
            "type": "string",
            "default": null,
            "format": "date",
            "example": "2006-01-01"
          },
          "blood": {
            "type": "number",
            "enum": [1, 2, 3, 4]
          },
          "sex": {
            "type": "string",
            "enum": ["male", "female"]
          },
          "levelActivity": {
            "type": "number",
            "enum": [1, 2, 3, 4, 5]
          }
        }
      },
      "Exercise": {
        "type": "object",
        "properties": {
          "bodyparts": {
            "type": "string"
          },
          "equipment": {
            "type": "string"
          },
          "gifUrl": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "target": {
            "type": "string"
          },
          "burnedCalories": {
            "type": "number"
          },
          "time": {
            "type": "number"
          }
        }
      },
      "Product": {
        "type": "object",
        "properties": {
          "weight": {
            "type": "number"
          },
          "calories": {
            "type": "number"
          },
          "category": {
            "type": "string"
          },
          "title": {
            "type": "string"
          },
          "groupBloodNotAllowed": {
            "type": "object",
            "properties": {
              "1": {
                "type": "boolean"
              },
              "2": {
                "type": "boolean"
              },
              "3": {
                "type": "boolean"
              },
              "4": {
                "type": "boolean"
              }
            }
          }
        }
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  }
}
